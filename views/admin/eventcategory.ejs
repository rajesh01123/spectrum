                      <input type="file" name="image"  class="form-control required" required>
                </div>
                   
                <div class="form-group  add-admin mt-3 col-md-12" style="margin-bottom: 0px;">
                    <input type="submit"  class="btn btn-primary" value="Submit"> 
                </div> 
                  </div>  
